@page
@using Server.Models
@model Server.Pages.SellAds

@{
    Layout = "/Pages/Shared/_Layout.cshtml";
}
@foreach (SellAd SellAd in Model.AllSellAds)
{
            <div class="sell-ad">
                <h3>@SellAd.Title</h3>
                <p>Beskrivning: @SellAd.Description</p>
                <p>Pris: @SellAd.Price</p>
                <p>Längd: @SellAd.Length </p>
            </div>
}

<!DOCTYPE html>
<html lang="en">

<head>
    <title>Sell Ad</title>
    <style>
        .sell-ad {
            border: 1px solid #ccc;
            padding: 10px;
            width: 300px;
            background-color: #f5f5f5;
            font-family: Arial, sans-serif;
        }
    </style>
</head>

<body>
    
</body>

</html>
